datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}
model OrderAccumulator{
  id String @id
  ativo  Ativo
  lado Lado
  quantidade Int
  preco  Float
  // @@map("orderaccumulators")
}
enum Ativo {
  PETR4
  VALE3
  VIIA4
}
enum Lado {
  C
  V
}
